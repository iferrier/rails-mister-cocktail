<div class="container">
  <h1>Create your new cocktail here :)</h1>

  <%= simple_form_for(@cocktail) do |f| %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Create" , class: 'btn btn-info' %>
  <% end %>
  <br>
  <p><%= link_to "Back to List", cocktails_path %></p>
</div>
